Programme {
	
	path="C:/Users/hp/Downloads/cars.csv"
	delimiter=";"
	keep(colName="model":string index= 0,colName="power":float index=4,colName="displacement":int index=3
	)
	where((power > Const int = "200" ) && (displacement > Const int = "100" ))  
	type = LineChart 
	title = "Power et déplacement des modèles de voiture ayant une puissance supérieure à 200 et un déplacement supérieur à 100"       
	xAxis = model          
	yAxis = (power,displacement)                    
	         
	     
	 
	 
	    
	}    