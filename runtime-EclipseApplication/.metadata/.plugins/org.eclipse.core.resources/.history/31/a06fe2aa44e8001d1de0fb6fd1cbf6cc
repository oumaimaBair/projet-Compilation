Programme {
	
	path="C:/Users/hp/Downloads/cars.csv"
	delimiter=";"
	keep(
		colName="model":string index= 0,
		colName="power":float index=4,
		colName="displacement":int index=3
	)
	where((power > Const int = "200" ) )  
	type = Doughnut  
	title = "Répartition des modèles de voitures ayant une puissance supérieure à 200"       
	xAxis = model          
	yAxis = (power)                        
	
	
	}      